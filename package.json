{
  "name": "chvn.me",
  "description": "Typescript, Next.js, Stitches, Radix Starter Project",
  "version": "1.0.0",
  "author": "chvndler <chan@ady.world> (@chv.ndler)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-hover-card": "^0.1.5",
    "@radix-ui/react-icons": "^1.1.0",
    "@radix-ui/react-id": "^0.1.5",
    "@radix-ui/react-select": "^0.1.1",
    "@radix-ui/react-separator": "^0.1.3",
    "@radix-ui/react-switch": "^0.1.4",
    "@radix-ui/react-toast": "^0.1.1",
    "@stitches/core": "1.2.6",
    "@stitches/react": "1.2.6",
    "animejs": "^3.2.1",
    "atelier.design": "^1.3.0",
    "boring-avatars": "^1.6.3",
    "date-fns": "^2.28.0",
    "hamburger-react": "^2.4.1",
    "inter-ui": "^3.19.3",
    "intersection-observer": "^0.12.0",
    "kbar": "^0.1.0-beta.33",
    "klaviyo-sdk": "^1.0.1",
    "motion": "^10.7.1",
    "next": "^12.1.4",
    "next-themes": "^0.0.15",
    "nextjs-progressbar": "^0.0.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.33.1",
    "stitches-mixins": "1.0.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@types/lodash.merge": "4.6.6",
    "@types/node": "16.11.22",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "^5.18.0",
    "compare-versions": "^4.1.3",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.3",
    "prettier": "^2.6.2",
    "rss": "^1.2.2",
    "typescript": "^4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "main": "next.config.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chvndler/chvn.me.git"
  },
  "bugs": {
    "url": "https://github.com/chvndler/chvn.me/issues"
  },
  "homepage": "https://github.com/chvndler/chvn.me#readme"
}
